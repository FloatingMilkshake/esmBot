{
  "name": "esmbot",
  "version": "2025.1.1",
  "description": "A Discord bot with miscellaneous features",
  "exports": "./dist/app.js",
  "bin": "./dist/app.js",
  "type": "module",
  "engines": {
    "node": ">=20.12.0"
  },
  "scripts": {
    "build": "tsc && pnpm build:natives",
    "build:debug": "tsc && pnpm build:natives:debug",
    "build:natives": "cmake-js compile --CDWITH_MAGICK=ON",
    "build:natives:debug": "cmake-js compile -BDebug --CDWITH_MAGICK=ON",
    "build:natives:no-magick": "cmake-js compile --CDWITH_MAGICK=OFF",
    "build:natives:debug-no-magick": "cmake-js compile -BDebug --CDWITH_MAGICK=OFF",
    "docker:build": "DOCKER_BUILDKIT=1 docker build -t esmbot .",
    "docker:run-bot": "docker run --rm --network=host esmbot",
    "docker:run-api": "docker run --rm --network=host --entrypoint='[\"node\", \"api/index.js\"]' esmbot",
    "docker:run-lava": "docker run --rm --network host -v \"$(pwd)\"/application.yml:/opt/Lavalink/application.yml ghcr.io/lavalink-devs/lavalink:4",
    "start": "node dist/app.js",
    "start:debug": "DEBUG=true node dist/app.js",
    "start-api": "node api/index.js",
    "start-api:debug": "DEBUG=true node api/index.js"
  },
  "author": "Essem <essem@essem.space>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/esmBot/esmBot.git"
  },
  "dependencies": {
    "better-sqlite3": "^11.8.1",
    "bufferutil": "^4.0.9",
    "dotenv": "^16.4.7",
    "emoji-regex-xs": "^1.0.0",
    "file-type": "^19.6.0",
    "format-duration": "^3.0.2",
    "ipaddr.js": "^2.2.0",
    "node-addon-api": "^8.3.0",
    "oceanic.js": "1.11.2",
    "postgres": "^3.4.5",
    "shoukaku": "^4.1.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/better-sqlite3": "^7.6.12",
    "@types/bun": "^1.2.2",
    "@types/node": "20",
    "@types/ws": "^8.5.14",
    "cmake-js": "^7.3.0",
    "typescript": "^5.7.3"
  },
  "optionalDependencies": {
    "@sentry/node": "^8.54.0",
    "@sentry/profiling-node": "^8.54.0",
    "pm2": "^5.4.3"
  },
  "binary": {
    "napi_versions": [
      7
    ]
  },
  "imports": {
    "#cmd-classes/*": {
      "default": "./src/classes/*",
      "node": "./dist/classes/*"
    },
    "#utils/*": {
      "default": "./src/utils/*",
      "node": "./dist/utils/*"
    },
    "#database": {
      "default": "./src/database.js",
      "node": "./dist/database.js"
    },
    "#pagination": {
      "default": "./src/pagination/pagination.js",
      "node": "./dist/pagination/pagination.js"
    },
    "#config/*": "./config/*"
  },
  "packageManager": "pnpm@10.2.0+sha1.5d36c5a1aae9a628a924ee9982f013ad202e1051",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@sentry/profiling-node",
      "@biomejs/biome",
      "better-sqlite3",
      "bufferutil"
    ],
    "patchedDependencies": {
      "bun-types": "patches/bun-types.patch"
    }
  }
}
